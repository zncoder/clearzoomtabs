{
  "name": "cleanzoomtabs",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Close zoom launch meeting tabs automatically",
	"permissions": [
		"tabs"
	],
	"icons": {
		"48": "icons/48.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": "icons/32.png",
		"default_title": "Clean Zoom launch meeting tabs"
	}
}
